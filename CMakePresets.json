{
   "version": 8,
   "configurePresets": [
      {
         "name": "local",
         "binaryDir": "${workspaceFolder}/build/",
         "cacheVariables": {
            "CMAKE_BUILD_TYPE": "Debug"
         }
      },
      {
         "name": "youbot",
         "displayName": "Configure preset using toolchain file",
         "binaryDir": "${workspaceFolder}/build",
         "cacheVariables": {
            "CMAKE_BUILD_TYPE": "Debug",
            "CMAKE_TOOLCHAIN_FILE": "${workspaceFolder}/youbot_toolchain.cmake",
            "CMAKE_INSTALL_PREFIX": "${workspaceFolder}/build/out"
         }
      }
   ]
}